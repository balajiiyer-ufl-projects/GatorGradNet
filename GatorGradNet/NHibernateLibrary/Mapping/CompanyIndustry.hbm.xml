<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="NhbHelper" assembly="NhbHelper">

  <class name="CompanyIndustry" table="COMPANYINDUSTRY">

    <composite-id>
      <key-property name="CompanyID" column="COMPANYID"></key-property>
      <key-property name="Industry" column="INDUSTRY" length="100"></key-property>
    </composite-id>

    <many-to-one name="CompanyID" class="Company" column="CompanyID" />

  </class>

</hibernate-mapping>