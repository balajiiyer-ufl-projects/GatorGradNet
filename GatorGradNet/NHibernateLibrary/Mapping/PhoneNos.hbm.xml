<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="NHibernateLibrary" assembly="NHibernateLibrary">

  <class name="PhoneNos" table="PHONENOS">

    <composite-id>
      <key-property name="UserID" column="USERID"></key-property>
      <key-property name="PhoneNo" column="PHONENO" length="20"></key-property>
    </composite-id>

    <many-to-one name="UserID" class="ProfileUser" column="USERID" />

  </class>

</hibernate-mapping>