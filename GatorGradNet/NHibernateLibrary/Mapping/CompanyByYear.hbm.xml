<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="NHibernateLibrary.Entities" assembly="NHibernateLibrary">

  <class name="CompanyByYear" table="COMPANYBYYEAR" mutable="false">

    <composite-id>
      <key-property name="CompanyID" column="COMPANYID" type="Int64"></key-property>
      <key-property name="Year" column="YEAR" type="Int32"></key-property>
    </composite-id>

    <property name="CompanyID" column="COMPANYID" type="Int64"></property>
    <property name="Year" column="YEAR" not-null="true" type="Int32"></property>
    <property name="TotalHires" column="TOTALHIRES" type="Int32"></property>

    </class>

</hibernate-mapping>